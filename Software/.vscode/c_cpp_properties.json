{
    "version": 4,
    "configurations": [
        {
            "name": "gcc",
            "intelliSenseMode": "gcc-arm",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "compilerPath": "${env:MBED_GCC_ARM_PATH}/arm-none-eabi-g++.exe",
            "forcedInclude": [
            ],
            "includePath": [
                "${workspaceFolder}",
                "Core/**",
                "Drivers/**",
                "lvFonts/**",
                "sh1107/**"
            ],
            "defines": [
                "DEBUG",
                "USE_HAL_DRIVER",
                "STM32G0B1xx",
                "LV_LVGL_H_INCLUDE_SIMPLE"
            ]
        }
    ]
}
